<view class="{{['cl-tabs','cl-tabs--'+type,isSticky]}}"><scroll-view class="cl-tabs__bar" style="{{'top:'+(stickyTop)+';'+('border-radius:'+(isSticky&&'none')+';')}}" scroll-with-animation="{{true}}" scroll-x="{{true}}" scroll-left="{{scrollLeft}}"><block wx:for="{{list2}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['change',[index]]]]]}}" class="{{['cl-tabs__bar-li',[(value==item.name)?'active':'']]}}" bindtap="__e"><view class="{{['active-background',[(value==item.name)?'active':'']]}}"><text class="cl-tabs__label">{{item.label}}</text></view><block wx:if="{{item.prefixIcon}}"><text class="{{['cl-tabs__icon','cl-tabs__icon--prefix',item.prefixIcon]}}"></text></block><block wx:if="{{item.suffixIcon}}"><text class="{{['cl-tabs__icon','cl-tabs__icon--suffix',item.suffixIcon]}}"></text></block></view></block></scroll-view><view class="cl-tabs__pane"><block wx:if="{{type==='swiper'}}"><swiper class="cl-tabs__swiper" circular="{{true}}" current="{{current}}" data-event-opts="{{[['change',[['onChangeSwiper',['$event']]]]]}}" bindchange="__e"><block wx:for="{{list2}}" wx:for-item="item" wx:for-index="index" wx:key="index"><swiper-item><scroll-view class="{{['cl-tabs__swiper-scroller']}}" scroll-y="{{true}}" enable-back-to-top="{{true}}"><block wx:if="{{item.visible}}"><scoped-slots-default item="{{item}}" index="{{index}}" bind:__l="__l"></scoped-slots-default></block></scroll-view></swiper-item></block></swiper></block><block wx:else><slot></slot></block></view></view>